{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_size": 14,
  "ui_font_family": "JetBrains Mono",
  "ui_font_size": 14,
  "vim_mode": true,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "cursor_blink": false,
  "scrollbar": {
    "show": "never"
  },
  "max_tabs": 5,
  "tab_bar": {
    "show": false
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": false
  },
  "project_panel": {
    "auto_fold_dirs": false
  },
  "features": {
    "inline_completion_provider": "none"
  },
  "theme": {
    "mode": "system",
    "dark": "Rosé Pine Moon",
    "light": "Rosé Pine Dawn"
  }
}
